{
    "Storage": "Primary",
    "EPP": 50,
    "Nodes":
    {
        "ID":
        {
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "Security.UID",
                    "Method": "Get",
                    "Call":
                    {
                        "Mode": "Secure"
                    }
                }
            }
        },
        "File":
        {

        },
        "Title":
        {
            "Type": "Literal.String",
            "Widgets":
            {
                "Write":
                {
                    "Type": "Form.Textfield"
                }
            }
        },
        "Meta.Package": {},
        "Meta":
        {
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "Package.Meta",
                    "Method": "Read"
                }
            }

        },
        "Name":
        {
            "Hooks":
            {
                "beforeEntityCreate":
                {
                    "Service": "Package.Name",
                    "Method": "Read"
                }
            }
        },
        "X-Private":
        {
            "Type": "Literal.String"
        },
        "Modified":
        {

        }
    },
    "Hooks":
    {
        "afterCreatePost":
        {
            "Redirect":
            {
                "Service": "System.Interface.Web",
                "Method": "Redirect",
                "Call":
                {
                    "Location": "/package/$Data.ID"
                }
            }
        }
    }
}