{
    "Links":
    {
        "/regen":
        {
            "Service": "Package.Directory",
            "Method": "Do"
        },
        "/binary/Packages":
        {
            "Service": "Package.List",
            "Method": "Generate",
            "Call":
            {
                "Renderer":
                {
                    "Service": "View.Plaintext",
                    "Method": "Render"
                }
            }
        },
         "/binary/Release":
        {
            "Service": "Package.Release",
            "Method": "Do",
            "Call":
            {
                "Renderer":
                {
                    "Service": "View.Plaintext",
                    "Method": "Render"
                }
            }
        },
        "/packages":
        {
            "Service": "Entity.List",
            "Method": "Do",
            "Call":
            {
                "Entity": "Package",
                "Where":
                {
                    "Meta.X-Private": "No"
                },
                "Sort":
                {
                    "Meta.Package": true
                }
            }
        }
    }
}